pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                sleep 2
            }
        }
        stage('build') {
            steps {
                echo 'Hello build'
                sleep 2
            }
        }
        
        stage('deploy') {
            steps {
                echo 'Hello deploy'
                sleep 2
            }
        }
        
        stage('test') {
            steps {
                echo 'Hello test'
            }
        }
        
    }
}


